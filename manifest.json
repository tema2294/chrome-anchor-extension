{
    "name": "Chrome SysUtils 2035",
    "description": "Helper tools for people who work with the sfera.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "64":"icons/64.png",
        "128":"icons/128.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["scripting","webNavigation", "activeTab", "contextMenus","tabCapture", "offscreen"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}